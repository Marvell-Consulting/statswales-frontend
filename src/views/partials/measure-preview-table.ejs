<%- include("./table", {
    columns: headers.map((col, index) => ({
        key: index,
        label: (t(`publish.measure_review.column_headers.${col.name.toLowerCase()}`) === `publish.measure_review.column_headers.${col.name.toLowerCase()}`)
            ? col.name
            : t(`publish.measure_review.column_headers.${col.name.toLowerCase()}`),
        format: (value, row) => {
            switch (col.name.toLowerCase()) {
                case 'start_date':
                case 'end_date': {
                    return locals.dateFormat(locals.parseISO(col.split('T')[0]), 'do MMMM yyyy')
                }
                case 'date_type': {
                    return t(`publish.measure_review.year_type.${col}`)
                }
            }
            if (value) {
                if (t(`publish.measure_review.column_values.${value.toString().toLowerCase()}`) === `publish.measure_review.column_values.${value.toString().toLowerCase()}`) {
                    return value
                } else {
                    return t(`publish.measure_review.column_values.${value.toString().toLowerCase()}`)
                }
            }
        }
    })),
    rows: data.filter(row => locals.langCol > -1 && row[locals.langCol] !== i18n.language.toLowerCase() ? false : true)
}); %>